pipeline{

    agent any
    
         stages{
            stage("Git Checkout"){
                steps{
                    echo "checkout completed"
                }
                }
                
                stage("Maven build"){
                def mvnhome = tool name: 'maven', type: 'maven'
                echo "build successfull"
                sh "${mvnhome}/bin/mvn package"
                 }
                stage("Test"){
                steps{
                echo "Testing successfull"
                }
                }
                stage("Deploy"){
                steps{
                echo "deployment is  successfull"
                }
                }
            }
            
              
            
            
        
    

}
