pipeline{

    agent {docker {image 'maven:3.3.9'}}
    stages{
   
         stage("Git Checkout"){
                steps{
                    echo "checkout completed"
                }
                }
                
                stage("Maven build"){
                steps{
                echo "build successfull"
               sh'mvn package'
                }
                 }
                stage("Test"){
                steps{
                echo "Testing successfull"
                }
                }
                stage("Deploy"){
                steps{
                echo "deployment is  successfull"
                }
                }
            }
            
              
            
            
        
    

}
