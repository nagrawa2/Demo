pipeline{

    agent any
    stages{
   
         stage("Git Checkout"){
                steps{
                    echo "checkout completed"
                }
                }
                
                stage("Maven build"){
                script{
                echo "build successfull"
                mvn package
                }
                 }
                stage("Test"){
                steps{
                echo "Testing successfull"
                }
                }
                stage("Deploy"){
                steps{
                echo "deployment is  successfull"
                }
                }
            }
            
              
            
            
        
    

}
